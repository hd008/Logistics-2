<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.logistics.entity.Record" table="record" schema="logistics">
        <id name="id">
            <column name="id" sql-type="tinyint(4)"/>
            <generator class="native"/>
        </id>
        <property name="orderId">
            <column name="order_id" sql-type="tinyint(4)"/>
        </property>
        <property name="buyerId">
            <column name="buyer_id" sql-type="tinyint(4)"/>
        </property>
        <property name="businessId">
            <column name="business_id" sql-type="tinyint(4)"/>
        </property>
    </class>
</hibernate-mapping>